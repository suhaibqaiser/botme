{
  "name": "botme-ws-server",
  "version": "1.0.0",
  "description": "BotMe WebSocket Server",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s && cp src/nlp/corpus-en.json ./build/nlp/",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/server.js",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --list-different \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "eslint --fix \"src/**/*.js\""
  },
  "author": "Aman Subhan <amansubhan929@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.6.0",
    "@nlpjs/basic": "^4.22.0",
    "axios": "^0.21.1",
    "compromise": "^13.10.6",
    "compromise-numbers": "^1.2.0",
    "mongoose": "^5.11.17",
    "stopword": "^1.0.7",
    "uuid": "^8.3.2",
    "ws": "7.4.3"
  }
}
